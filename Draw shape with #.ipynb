    "#from array import *\n",
    "arr = []\n",
    "string = ''\n",
    "n = int(input())\n",
    "j = 1\n",
    "while j < n+1:\n",
    "    i = j\n",
    "    while n-i:\n",
    "        arr.append(\" \")\n",
    "        i+= 1\n",
    "    col = j\n",
    "    while col:\n",
    "        arr.append(\"#\")\n",
    "        col-= 1\n",
    "    j+=1\n",
    "    string = ''.join(arr)\n",
    "    print(string)\n",
    "    arr = []"
